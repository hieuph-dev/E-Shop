{
  "version": 2,
  "name": "e-shop",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node vercel-sapper",
      "config": {
        "build": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
    }
  ]
}
